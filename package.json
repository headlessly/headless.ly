{
  "name": "@headlessly/sdk",
  "version": "0.0.1",
  "description": "Headless SaaS Platform for AI Agents",
  "type": "module",
  "main": "index.js",
  "types": "types.d.ts",
  "bin": {
    "headlessly": "./cli.js"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./types.d.ts"
    },
    "./nouns/*": {
      "import": "./nouns/*/index.js",
      "types": "./nouns/*/types.d.ts"
    }
  },
  "files": [
    "index.js",
    "types.d.ts",
    "cli.js",
    "nouns/",
    "README.md"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "vitest --typecheck"
  },
  "dependencies": {
    "rpc.do": "^0.1.0",
    "oauth.do": "^0.1.0",
    "cli.do": "^0.1.0",
    "mcp.do": "^0.1.0"
  },
  "devDependencies": {
    "vitest": "^3.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "headless",
    "saas",
    "ai",
    "agents",
    "rpc"
  ],
  "author": "headless.ly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/headlessly/headless.ly"
  },
  "homepage": "https://headless.ly",
  "publishConfig": {
    "access": "public"
  }
}
