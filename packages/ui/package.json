{
  "name": "@headlessly/ui",
  "version": "0.1.0",
  "description": "Schema-driven React CRUD components for headless.ly entities",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "digital-objects": ">=1.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "digital-objects": "workspace:*",
    "jsdom": "^25.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.7.2",
    "vitest": "^3.0.2"
  },
  "keywords": [
    "headlessly",
    "react",
    "crud",
    "entity",
    "schema-driven",
    "ui",
    "components"
  ],
  "author": "headless.ly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/headlessly/headless.ly"
  },
  "homepage": "https://headless.ly"
}
