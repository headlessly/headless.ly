    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^3.0.2"
  },
  "keywords": [
    "headlessly",
    "events",
    "event-sourcing",
    "time-travel",
    "cdc",
    "agent-first"
  ],
  "author": "headless.ly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/headlessly/headless.ly",
    "directory": "public/packages/events"
  },
  "homepage": "https://headless.ly"
}
