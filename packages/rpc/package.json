  "keywords": [
    "headlessly",
    "rpc",
    "capnweb",
    "promise-pipelining",
    "rpc.do"
  ],
  "author": "headless.ly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/headlessly/headless.ly",
