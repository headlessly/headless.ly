{
  "name": "headless.ly",
  "version": "0.1.0",
  "description": "The Operating System for Agent-First Startups â€” SDK entry point",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./crm": {
      "import": "./dist/domains/crm.js",
      "types": "./dist/domains/crm.d.ts"
    },
    "./billing": {
      "import": "./dist/domains/billing.js",
      "types": "./dist/domains/billing.d.ts"
    },
    "./projects": {
      "import": "./dist/domains/projects.js",
      "types": "./dist/domains/projects.d.ts"
    },
    "./content": {
      "import": "./dist/domains/content.js",
      "types": "./dist/domains/content.d.ts"
    },
    "./support": {
      "import": "./dist/domains/support.js",
      "types": "./dist/domains/support.d.ts"
    },
    "./analytics": {
      "import": "./dist/domains/analytics.js",
      "types": "./dist/domains/analytics.d.ts"
    },
    "./marketing": {
      "import": "./dist/domains/marketing.js",
      "types": "./dist/domains/marketing.d.ts"
    },
    "./experiments": {
      "import": "./dist/domains/experiments.js",
      "types": "./dist/domains/experiments.d.ts"
    },
    "./platform": {
      "import": "./dist/domains/platform.js",
      "types": "./dist/domains/platform.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessly/sdk": "workspace:*",
    "@headlessly/objects": "workspace:*",
    "@headlessly/crm": "workspace:*",
    "@headlessly/billing": "workspace:*",
    "@headlessly/projects": "workspace:*",
    "@headlessly/content": "workspace:*",
    "@headlessly/support": "workspace:*",
    "@headlessly/analytics": "workspace:*",
    "@headlessly/marketing": "workspace:*",
    "@headlessly/experiments": "workspace:*",
    "@headlessly/platform": "workspace:*",
    "digital-objects": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^3.0.2"
  },
  "keywords": [
    "headlessly",
    "headless.ly",
    "sdk",
    "agent-first",
    "digital-objects",
    "crm",
    "billing",
    "projects"
  ],
  "author": "headless.ly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/headlessly/headless.ly",
    "directory": "packages/headlessly"
  },
  "homepage": "https://headless.ly"
}
