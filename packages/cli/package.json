{
  "name": "@headlessly/cli",
  "version": "0.1.0",
  "description": "CLI tool for headless.ly developers and agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "headlessly": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:e2e": "vitest run --config ../vitest.e2e.config.ts",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "digital-objects": "workspace:*",
    "@headlessly/sdk": "workspace:*",
    "@headlessly/objects": "workspace:*",
    "@headlessly/mcp": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "typescript": "^5.7.2",
    "vitest": "^3.0.2"
  },
  "keywords": [
    "headlessly",
    "cli",
    "agent-first",
    "digital-objects"
  ],
  "author": "headless.ly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/headlessly/headless.ly",
    "directory": "public/packages/cli"
  },
  "homepage": "https://headless.ly"
}
