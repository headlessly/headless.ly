{
  "name": "@headlessly/cli",
  "version": "0.0.1",
  "description": "CLI tool for headless.ly developers and agents",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "headlessly": "./dist/bin.js"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "digital-objects": "workspace:*",
    "@headlessly/sdk": "workspace:*",
    "@headlessly/objects": "workspace:*",
    "@headlessly/mcp": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "typescript": "^5.7.2",
    "vitest": "^3.0.2"
  },
  "keywords": [
    "headlessly",
    "cli",
    "agent-first",
    "digital-objects"
  ],
  "author": "headless.ly",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  }
}
